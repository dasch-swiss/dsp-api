###  http://www.knora.org/ontology/knora-base#attachedToProject

:attachedToProject rdf:type owl:ObjectProperty ;
                   
                   rdfs:comment "Connects a Resource or a Value to a project." ;
                   
                   rdfs:range :knoraProject ;
                   
                   rdfs:domain [ rdf:type owl:Class ;
                                 owl:unionOf ( :Resource
                                               :User
                                               :Value
                                             )
                               ] .



###  http://www.knora.org/ontology/knora-base#attachedToUser

:attachedToUser rdf:type owl:ObjectProperty ;
                
                rdfs:comment "Connects a Resource or a Value to a user." ;
                
                rdfs:range :User ;
                
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( :Resource
                                            :Value
                                          )
                            ] .



###  http://www.knora.org/ontology/knora-base#belongsTo

:belongsTo rdf:type owl:ObjectProperty ;
           
           rdfs:comment "This property indicates, to which Institution  a knoraProject belongs to." ;
           
           rdfs:range :Institution ;
           
           rdfs:domain :knoraProject .



###  http://www.knora.org/ontology/knora-base#currentproject

:currentproject rdf:type owl:ObjectProperty ;
                
                rdfs:comment "This property indicates, which is the \"current project\" of a given user. A user can be part of any number of projects, but only one can be the active project." ;
                
                rdfs:domain :User ;
                
                rdfs:range :knoraProject .



