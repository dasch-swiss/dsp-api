###  http://www.knora.org/ontology/knora-base#protocol

:protocol rdf:type owl:DatatypeProperty ;
          
          rdfs:range [ rdf:type rdfs:Datatype ;
                       owl:oneOf [ rdf:type rdf:List ;
                                   rdf:first "file" ;
                                   rdf:rest [ rdf:type rdf:List ;
                                              rdf:first "ftp" ;
                                              rdf:rest [ rdf:type rdf:List ;
                                                         rdf:first "http" ;
                                                         rdf:rest [ rdf:type rdf:List ;
                                                                    rdf:first "https" ;
                                                                    rdf:rest [ rdf:type rdf:List ;
                                                                               rdf:first "sftp" ;
                                                                               rdf:rest [ rdf:type rdf:List ;
                                                                                          rdf:first "soap" ;
                                                                                          rdf:rest [ rdf:type rdf:List ;
                                                                                                     rdf:first "xmlrpc" ;
                                                                                                     rdf:rest rdf:nil
                                                                                                   ]
                                                                                        ]
                                                                             ]
                                                                  ]
                                                       ]
                                            ]
                                 ]
                     ] .



