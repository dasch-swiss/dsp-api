###  http://www.knora.org/ontology/knora-base#Institution

:Institution rdf:type owl:Class ;
             
             rdfs:subClassOf <http://xmlns.com/foaf/0.1/Organization> ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :website ;
                               owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
                               owl:onDataRange xsd:string
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :phone ;
                               owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
                               owl:onDataRange xsd:string
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :address ;
                               owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
                               owl:onDataRange xsd:string
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :email ;
                               owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
                               owl:onDataRange xsd:string
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty <http://xmlns.com/foaf/0.1/name> ;
                               owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onDataRange xsd:string
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :description ;
                               owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
                               owl:onDataRange xsd:string
                             ] ;
             
             rdfs:comment "An Institution is an academic (or similar) entity that has assets in Knora"@en .



