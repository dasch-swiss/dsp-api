                     rdfs:label "Repräsentation (Audio)"@de ,
                                "Representation (Audio)"@en ,
                                "Répresentation (Audio)"@fr ,
                                "Rappresentazione (Audio)"@it ;
                     
                     rdfs:comment "Represents a file containing audio data"@en .



