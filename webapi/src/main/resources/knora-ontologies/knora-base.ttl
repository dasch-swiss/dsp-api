    :subjectClassConstraint :Region ;
    rdfs:subPropertyOf      :hasValue ;
    :isEditable             true ;
    salsah-gui:guiElement   salsah-gui:Colorpicker .


###  http://www.knora.org/ontology/knora-base#hasComment

:hasComment
    rdf:type                  owl:ObjectProperty ;
    rdfs:label                "Kommentar"@de,
                              "Comment"@en,
                              "Commentaire"@fr,
                              "Commento"@it ;
