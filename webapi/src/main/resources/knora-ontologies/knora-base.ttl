###  http://www.knora.org/ontology/knora-base#hasColor

:hasColor
    rdf:type                owl:ObjectProperty ;
    rdfs:label              "Farbe"@de,
                            "Color"@en,
                            "Couleur"@fr,
                            "Colore"@it ;
    rdfs:comment            """Specifies the color of a region."""@en ;
    salsah-gui:guiAttribute "ncolors=8"^^xsd:string ;
