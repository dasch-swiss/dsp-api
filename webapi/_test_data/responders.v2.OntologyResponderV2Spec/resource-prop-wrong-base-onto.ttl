@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix knora-base: <http://www.knora.org/ontology/knora-base#> .
@prefix salsah-gui: <http://www.knora.org/ontology/salsah-gui#> .
@base <http://www.knora.org/ontology/invalid> .

# An ontology that contains a resource property with a base property that has a Knora IRI but isn't a Knora resource property.

@prefix : <http://www.knora.org/ontology/invalid#> .
<http://www.knora.org/ontology/invalid> rdf:type owl:Ontology ;
    rdfs:label "The invalid ontology" ;
    knora-base:attachedToProject <http://rdfh.ch/projects/anything> .


:hasFoo rdf:type owl:ObjectProperty ;

               rdfs:subPropertyOf knora-base:hasValue, knora-base:deleteDate ; 

               rdfs:label "has foo" ;

		       knora-base:objectClassConstraint knora-base:TextValue .
