#
# CLASSES
#

philo:Philosophy
	a owl:Class;
	rdfs:label "philosophy"@en, "Philosophie"@de;
	rdfs:comment """."""@en; #*
	rdfs:subClassOf concept:ThoughtBody.

philo:Metaphysics
	a owl:Class;
	rdfs:label "metaphysics"@en, "Metaphysik"@de;
	rdfs:comment """."""@en; #*
	rdfs:subClassOf philo:Philosophy. #*

