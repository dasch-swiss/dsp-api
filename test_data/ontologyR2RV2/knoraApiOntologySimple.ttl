        rdfs:comment          "Represents an interval." ;
        rdfs:label            "Interval literal" ;
        owl:onDatatype        xsd:string ;
        owl:withRestrictions  [ xsd:pattern  "\\d+(\\.\\d+)?,\\d+(\\.\\d+)?" ] .

